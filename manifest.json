{
    "manifest_version": 2,
    "name": "instagramUnfollow",
    "description": "",
    "version": "0.1",
    "permissions": ["activeTab"],
    "icons": { 
        "48": "icons/instagram.png"
},

    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": [
                "content.js"
            ]
        }
    ],
    "web_accessible_resources": [
        "usernames.txt"
      ]
}